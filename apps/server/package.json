{
  "name": "server",
  "private": true,
  "sideEffects": false,
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm run db:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:prod": "prisma migrate deploy && node dist/src/main",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../../node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "db:generate": "pnpx prisma generate",
    "db:migrate": "pnpx prisma migrate dev",
    "db:seed": "pnpx prisma migrate reset --force",
    "docker": "docker-compose up",
    "postinstall": "pnpm run db:generate"
  },
  "dependencies": {
    "@casl/ability": "^5.4.4",
    "@devoxa/prisma-relay-cursor-connection": "^2.2.1",
    "@nestjs/cli": "^9.1.9",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "4.8.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "googleapis": "^105.0.0",
    "helmet": "^5.0.2",
    "nestjs-slack": "^2.0.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prisma": "4.8.0",
    "prisma-offset-pagination": "^0.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.7",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@swc/jest": "^0.2.23",
    "@types/cron": "^2.0.0",
    "@types/jest": "^28.1.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "esbuild-jest": "^0.5.0",
    "jest": "^28.0.3",
    "jest-mock-extended": "^3.0.1",
    "prisma-erd-generator": "^1.2.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^4.0.0"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
